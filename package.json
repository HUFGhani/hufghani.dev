{
  "name": "new.hufghani.dev",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "sst bind vite dev",
    "build": "sh ./patch-apollo-client.sh && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "cm": "cz",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest src --config jest.config.json",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest src --config jest.config.json --watch",
    "sst:deploy": "sst deploy",
    "sst:dev": "sst dev"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@commitlint/cli": "^17.6.6",
    "@sveltejs/kit": "1.3.6",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@types/jest": "^29.5.3",
    "@types/testing-library__jest-dom": "^5.14.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.14",
    "aws-cdk-lib": "^2.84.0",
    "babel-jest": "^29.6.1",
    "commitizen": "^4.3.0",
    "commitlint-config-gitmoji": "^2.3.1",
    "constructs": "10.1.156",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "postcss": "^8.4.25",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "sst": "^2.19.2",
    "svelte": "^3.59.2",
    "svelte-check": "^3.4.5",
    "svelte-jester": "^2.3.2",
    "svelte-kit-sst": "^2.19.2",
    "svelte-meta-tags": "^2.8.0",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.0",
    "typescript": "~4.9.5",
    "vite": "^4.4.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "daisyui": "^2.52.0",
    "devicon": "^2.15.1",
    "graphql": "^16.7.1",
    "graphql-tag": "^2.12.6"
  }
}
